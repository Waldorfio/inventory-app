<h1>This a test page to display all games</h1>
<h2>Games:</h2>
<p></p>
<% for(const game of games) { %>
  <div class='game-container'>
    <h3><%= game.title%></h3>
    <p><%= game.summary%></p>
    <h4><%= game.edition%></h4>
    <h4><<%= game.price%></h4>
    <h4><%= game.discount%></h4>
    <p><i><%= game.release%></i></p>
    <a href='/game/game.id/update'>Edit</a>
    <a href='/game/game.id/delete'>Delete</a>
  </div>
<% } %>

<a href="/">return home</a>
